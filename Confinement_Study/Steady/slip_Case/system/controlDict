FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
application     N-S-PNPDImlessFoam;
startFrom       latestTime; // startTime;
startTime       0;
stopAt          endTime;
endTime         100; // 1;
deltaT          1;
writeControl    timeStep;
writeInterval   1;
purgeWrite      0;
writeFormat     ascii;
writePrecision  6;
writeCompression off;
timeFormat      general;
timePrecision   6;
runTimeModifiable true;
libs ("libfixedIonicFlux.so" "libmodifiedFixedValue.so" "libslipSmoluchowski.so" "libzeroIonicFlux_nonD.so");
nNewtonIter	50; //15;
nNewtonIterPNP	150;
nPNPNSIter	500;
C1C2convergence	1e-3; //1e-3;
PNPNStransient no;
solveNS no; // yes;
pseudoTransient yes;
splitpsi no;
saveVtkFiles no;
saveDesaltPhysics no;
debugMode no;
